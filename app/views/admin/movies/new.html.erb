<%=form_with model: @movie,url: "/admin/movies" do |f|%>
  <%=f.label :タイトル%>
  <%= f.text_field  :name %>
  <br>
  <%=f.label :公開年%>
  <%= f.text_field  :year %>
  <br>
  <%=f.label :詳細%>
  <%= f.text_area :description %>
  <br>
  <%=f.label :画像URL%>
  <%= f.text_field :image_url %>
  <br>
  <%=f.label :公開中%>
  <%= f.check_box  :is_showing %>
  <%= f.submit "Submit" %>
<%end%>


